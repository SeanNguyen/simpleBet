{
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
        "gen": "Microsoft.Framework.CodeGeneration",
        "ef": "EntityFramework.Commands"
    },
    "dependencies": {
        "EntityFramework": "6.1.3",
        //"EntityFramework.Commands": "7.0.0-beta4",
        //"EntityFramework.SqlServer": "7.0.0-beta4",
        "Microsoft.AspNet.Mvc": "6.0.0-beta4",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta4",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta4",
        "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta4",
        "Microsoft.Framework.ConfigurationModel.Interfaces": "1.0.0-beta4",
        "System.Data.SqlClient": "4.0.0-beta-22816",
        "Microsoft.Framework.ConfigurationModel": "1.0.0-beta4",
        "Newtonsoft.Json": "6.0.6",
        "Microsoft.AspNet.Mvc.WebApiCompatShim": "6.0.0-beta4"
    },
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ],
    "frameworks": {
        "dnx451": { }
        //Disable dnxcore50 because it is not supported by EF6
        //"dnxcore50": { }
    },
    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ],
    "version": "1.0.0-*",
    "webroot": "wwwroot"
}
